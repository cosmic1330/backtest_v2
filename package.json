    "lint-staged": "^12.1.4",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.62.0",
    "typescript": "^4.5.4",
    "vite": "^2.7.9",
    "vite-tsconfig-paths": "^3.3.17",
